{
    "name": "aklump/live-dev-porter",
    "description": "Simplifies the management and transfer of assets between website environments.",
    "license": "BSD-3-Clause",
    "autoload": {
        "psr-4": {
            "AKlump\\LiveDevPorter\\": "src/",
            "AKlump\\Cloudy\\": "./cloudy/php/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AKlump\\LiveDevPorter\\Tests\\": "tests_phpunit/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "aklump/phpswap": "^0.0.6"
    },
    "bin": [
        "bin/ldp"
    ],
    "require": {
        "aklump/build-url": "^0.0.2",
        "aklump/glob": "^0.0",
        "aklump/loft-lib": "^2.1",
        "ckr/arraymerger": "^2.0",
        "doctrine/lexer": "^1.2 || ^2.1",
        "ext-json": "*",
        "jasny/dotkey": "^1.0",
        "justinrainbow/json-schema": "^5.2",
        "nikic/php-parser": "^4.15",
        "php": "^7.3 || ^8",
        "symfony/yaml": "^3.4 || ^4 || ^5 || ^6"
    }
}
