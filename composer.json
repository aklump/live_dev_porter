{
    "name": "aklump/live-dev-porter",
    "description": "Simplifies the management and transfer of assets between website environments.",
    "bin": [
        "bin/live-dev-porter",
        "bin/ldp"
    ],
    "license": "BSD-3-Clause",
    "autoload": {
        "psr-4": {
            "AKlump\\LiveDevPorter\\": "src"
        }
    },
    "require": {
        "symfony/yaml": "^3.4|^4|^5",
        "justinrainbow/json-schema": "^5.2",
        "aklump/data": "^0.0.28",
        "aklump/loft-lib": "^1.1",
        "jasny/dotkey": "^1.0",
        "ckr/arraymerger": "^2.0"
    },
    "scripts": {
        "post-install-cmd": [
            "live-dev-porter init"
        ],
        "pre-uninstall-cmd": [
            "AKlump\\LiveDevPorter\\Composer::uninstall"
        ]
    }
}
